{
  "name": "back-end",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "engines":{
    "node": "14.x"
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "node index.js"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mongoose": "^8.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "type": "module"
}
