{
  "name": "back-end",
  "version": "0.0.1",
  "private": true,

  "main": "src/bin/server.js",


"builds": [
    {
      "src": "src/bin/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/bin/server.js"
    }
  ],



  "scripts": {
    "dev": "nodemon ./src/bin/server.js",
    "start": "node ./src/bin/server.js"
  },
  
  "dependencies": {
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mongoose": "^8.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "type": "module"
}
